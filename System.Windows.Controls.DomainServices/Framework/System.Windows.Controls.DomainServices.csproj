  <!-- ReplaceKeysInFiles target replaces RiaServicesKey in some source files -->
  <Target Name="ReplaceKeysInFiles">
    <!-- The files containing the RiaServicesKey in text -->
    <ItemGroup>
      <FilesWithKeys Include="System.Windows.Controls.DomainServices.extmap.xml">
      </FilesWithKeys>
    </ItemGroup>
    <Copy SourceFiles="@(FilesWithKeys)" DestinationFiles="$(OutputPath)%(FilesWithKeys.FileName)%(FilesWithKeys.Extension)" OverwriteReadOnlyFiles="true" />
    <ItemGroup>
      <BinFilesWithKeys Include="$(OutputPath)%(FilesWithKeys.FileName)%(FilesWithKeys.Extension)">
      </BinFilesWithKeys>
    </ItemGroup>
    <Message Text="ReplaceKeysInFiles inserting key $(RiaServicesKey) into @(BinFilesWithKeys)" />
    <FileUpdate Files="@(BinFilesWithKeys)" Regex="{RiaServicesKey}" ReplacementText="$(RiaServicesKey)" />
  </Target>
